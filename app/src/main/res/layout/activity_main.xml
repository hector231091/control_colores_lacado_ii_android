            android:layout_height="50dp"
            android:layout_marginTop="2dp"
            android:weightSum="6">

            <TextView
                android:id="@+id/ultimo_color_fila_2"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
