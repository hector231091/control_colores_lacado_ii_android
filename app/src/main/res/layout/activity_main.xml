<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:context=".MainActivity"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="65dp"
            android:layout_marginTop="10dp"
            android:weightSum="6">

            <TextView
                android:id="@+id/colur_header"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/config_header_input_data"
                android:gravity="center"
                android:text="COLOR"
                android:textColor="@color/black"
                android:textSize="20dip" />

            <Button
                android:id="@+id/change_start_time_button"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:background="@drawable/config_header_input_data"
                android:layout_marginLeft="5dp"
                android:textSize="20dip"
                android:text="INICIO CAMBIO"/>

            <Button
                android:id="@+id/colour_start_time_button"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:background="@drawable/config_header_input_data"
                android:layout_marginLeft="5dp"
                android:textSize="20dip"
                android:text="INICIO COLOR"/>

            <Button
                android:id="@+id/colour_end_time_button"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:background="@drawable/config_header_input_data"
                android:layout_marginLeft="5dp"
                android:textSize="20dip"
                android:text="FINAL COLOR"/>

            <TextView
                android:id="@+id/hangers_header"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textColor="@color/black"
                android:background="@drawable/config_header_input_data"
                android:layout_marginLeft="5dp"
                android:textSize="20dip"
                android:text="BASTIDORES"/>

            <TextView
                android:id="@+id/observations_header"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textColor="@color/black"
                android:background="@drawable/config_header_input_data"
                android:layout_marginLeft="5dp"
                android:textSize="20dip"
                android:text="OBSERVACIONES"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="65dp"
            android:layout_marginTop="10dp"
            android:weightSum="6">

            <EditText
                android:id="@+id/colour_entry"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:background="@drawable/border_edit_text_and_text_view"/>

            <TextView
                android:id="@+id/change_start_time_label"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/border_edit_text_and_text_view"/>

            <TextView
                android:id="@+id/colour_start_time_label"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/border_edit_text_and_text_view"/>

            <TextView
                android:id="@+id/colour_end_time_label"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/border_edit_text_and_text_view"/>

            <EditText
                android:id="@+id/hangers_entry"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:inputType="number"
                android:layout_marginLeft="5dp"
                android:background="@drawable/border_edit_text_and_text_view"/>

            <EditText
                android:id="@+id/observations_entry"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:background="@drawable/border_edit_text_and_text_view"
                android:layout_marginLeft="5dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="90dp"
            android:layout_marginTop="20dp"
            android:weightSum="3">

            <Button
                android:id="@+id/register_stop_button"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="30dip"
                android:text="REGISTRAR Y DESCANSO"/>

            <Button
                android:id="@+id/register_continue_button"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:textSize="30dip"
                android:text="REGISTRAR Y CONTINUAR"/>

            <Button
                android:id="@+id/register_end_button"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:textSize="30dip"
                android:text="REGISTRAR Y FINALIZAR"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="60dp"
            android:layout_marginTop="20dp"
            android:weightSum="6">
            <TextView
                android:id="@+id/colur_header_historic"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
