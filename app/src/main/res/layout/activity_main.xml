        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="40dp"
            android:layout_marginTop="20dp"
            android:weightSum="1">

            <TextView
                android:id="@+id/errores"
                android:layout_weight="1"
                android:layout_width="0dp"
