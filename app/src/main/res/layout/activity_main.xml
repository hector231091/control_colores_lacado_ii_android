                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/border_edit_text_and_text_view"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:layout_marginTop="2dp"
            android:weightSum="6">

            <TextView
                android:id="@+id/ultimo_color_fila_3"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:background="@drawable/border_edit_text_and_text_view"/>

            <TextView
                android:id="@+id/ultimo_hora_inicio_cambio_fila_3"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/border_edit_text_and_text_view"/>

            <TextView
                android:id="@+id/ultimo_hora_inicio_color_fila_3"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/border_edit_text_and_text_view"/>

            <TextView
                android:id="@+id/ultimo_hora_final_color_fila_3"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/border_edit_text_and_text_view"/>

            <TextView
                android:id="@+id/ultimo_bastidores_fila_3"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/border_edit_text_and_text_view"/>

            <TextView
                android:id="@+id/ultimo_observaciones_fila_3"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:background="@drawable/border_edit_text_and_text_view"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:layout_marginTop="2dp"
            android:weightSum="6">

            <TextView
                android:id="@+id/ultimo_color_fila_4"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
